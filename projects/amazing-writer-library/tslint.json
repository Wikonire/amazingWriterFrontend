{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "aw",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "aw",
      "kebab-case"
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "no-shadowed-variable": false
  }
}
